<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:padding="0dp"
    tools:context=".Activities.LogInActivity">

    <Button
        android:id="@+id/buttonMilestones8"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_below="@+id/textViewSettings2"
        android:layout_alignBottom="@+id/indicator2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="#5DAD75"
        android:text="Add"
        android:textSize="25dp"
        app:iconTint="#5DAD75" />

    <Button
        android:id="@+id/buttonMilestones9"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_alignTop="@+id/buttonMilestones8"
        android:layout_alignBottom="@+id/buttonMilestones8"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="0dp"
        android:backgroundTint="#5DAD75"
        android:text="Remove"
        android:textSize="25dp"
        app:iconTint="#5DAD75" />

    <TextView
        android:id="@+id/textViewSettings4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="10dp"
        android:background="#5DAE75"
        android:fontFamily="sans-serif-medium"
        android:paddingTop="10dp"
        android:paddingBottom="5dp"
        android:text="Currency"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageViewButtonReturn3"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/img_close_black_900" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="173dp"
        android:layout_height="50dp"
        android:layout_above="@+id/buttonMilestones8"
        android:layout_below="@+id/editTextUsername9"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="0dp" />

    <EditText
        android:id="@+id/editTextUsername9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewSettings4"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:hint="Search"
        android:inputType="text"
        android:paddingBottom="20dp" />

    <Button
        android:id="@+id/indicator2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_above="@+id/indicator"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="630dp"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="#C4C4C4" />

    <TextView
        android:id="@+id/textViewLogout"
        android:layout_width="87dp"
        android:layout_height="46dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:fontFamily="sans-serif-medium"
        android:gravity="center"
        android:text="Filters"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="16dp" />

    <ImageView
        android:id="@+id/imageViewButtonProfile"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="165dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="0dp"
        app:srcCompat="@drawable/img_avatar1" />

    <ImageView
        android:id="@+id/imageViewButtonBuyingIn"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="300dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="0dp"
        app:srcCompat="@drawable/img_company1" />

    <ImageView
        android:id="@+id/imageViewButtonRecycle"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="300dp"
        android:layout_marginBottom="0dp"
        app:srcCompat="@android:drawable/presence_invisible" />

    <Button
        android:id="@+id/indicator"
        android:layout_width="49dp"
        android:layout_height="15dp"
        android:layout_alignTop="@+id/imageViewButtonRecycle"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="67dp"
        android:layout_marginTop="-10dp"
        android:layout_marginEnd="317dp"
        android:backgroundTint="#5DAD75" />

</RelativeLayout>